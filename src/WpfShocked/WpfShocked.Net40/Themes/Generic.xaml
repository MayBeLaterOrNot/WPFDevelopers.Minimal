			<Border BorderBrush="{StaticResource PrimaryPressedSolidColorBrush}" BorderThickness="2" Background="{StaticResource WhiteSolidColorBrush}" SnapsToDevicePixels="True" Height="{StaticResource ThumbWidth}" Width="{StaticResource ThumbWidth}" CornerRadius="8" Name="PART_Border" RenderTransformOrigin=".5,.5" />
		</Grid>
		<ControlTemplate.Triggers>
