										<DoubleAnimation Storyboard.TargetName="PART_InderminateMark" Storyboard.TargetProperty="(Path.RenderTransform).(ScaleTransform.ScaleX)" To="1" Duration="00:00:.2" EasingFunction="{StaticResource ExponentialEaseOut}" />
									</Storyboard>
